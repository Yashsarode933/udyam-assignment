generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Submission {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  step      Int

  aadhaar   String?
  mobile    String?
  otp       String?

  pan       String?
  pincode   String?
  state     String?
  city      String?

  rawJson   String
}

